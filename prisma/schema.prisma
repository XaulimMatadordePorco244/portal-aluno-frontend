
datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL") 
}

generator client {
  provider = "prisma-client-js"
}



model User {
  id        String   @id @default(cuid()) 
  cpf       String   @unique 
  password  String   
  nome      String
  numero    String?  @unique 
  cargo     String   @default("Aluno Soldado")
  

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}